<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<?eclipse version="3.0"?><plugin>

   <!-- This is an example for providing an own category 
      <extension
         point="org.knime.workbench.repository.categories">
      <category
            description="Category containing my nodes"
            icon="icons/default.png"
            level-id="myNodes"
            name="My nodes"
            path="/"/>
   </extension>
   -->
   
   <extension point="org.knime.workbench.repository.nodes">
      <node category-path="/pmm" factory-class="de.bund.bfr.knime.pmm.util.pmmtoxml.PmmToXmlNodeFactory" id="de.bund.bfr.knime.pmm.util.pmmtoxml.PmmToXmlNodeFactory"/>
   <node
         category-path="/pmm"
         factory-class="de.bund.bfr.knime.pmm.util.join.JoinerNodeFactory"
         id="de.bund.bfr.knime.pmm.util.join.JoinerNodeFactory">
   </node>
   <node
         category-path="/pmm"
         factory-class="de.bund.bfr.knime.pmm.util.fitting.ModelFittingNodeFactory"
         id="de.bund.bfr.knime.pmm.util.fitting.ModelFittingNodeFactory">
   </node>
</extension>
   
</plugin>
