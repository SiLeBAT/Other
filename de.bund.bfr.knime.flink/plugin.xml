<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<?eclipse version="3.0"?><plugin>

   <!-- This is an example for providing an own category 
      <extension
         point="org.knime.workbench.repository.categories">
      <category
            description="Category containing my nodes"
            icon="icons/default.png"
            level-id="myNodes"
            name="My nodes"
            path="/"/>
   </extension>
   -->
   
   <extension point="org.knime.workbench.repository.nodes">
   <node category-path="/flink" factory-class="de.bund.bfr.knime.flink.scala.FlinkScalaBuilderNodeFactory" id="de.bund.bfr.knime.flink.scala.FlinkScalaBuilderNodeFactory"/>
<node category-path="/flink" factory-class="de.bund.bfr.knime.flink.jm.FlinkJobManagerConnectionNodeFactory" id="de.bund.bfr.knime.flink.jm.FlinkJobManagerConnectionNodeFactory"/>
<node category-path="/flink" factory-class="de.bund.bfr.knime.flink.jm.FlinkJobSubmissionNodeFactory" id="de.bund.bfr.knime.flink.jm.FlinkJobSubmissionNodeFactory"/>
<node category-path="/flink" factory-class="de.bund.bfr.knime.flink.program.FlinkProgramLoaderNodeFactory" id="de.bund.bfr.knime.flink.program.FlinkProgramLoaderNodeFactory"/>
<node category-path="/hdfs" factory-class="de.bund.bfr.knime.hdfs.file.HDFSUploadNodeFactory" id="de.bund.bfr.knime.hdfs.file.HDFSUploadNodeFactory"/>
<node category-path="/hdfs" factory-class="de.bund.bfr.knime.hdfs.connection.HDFSConnectionNodeFactory" id="de.bund.bfr.knime.hdfs.connection.HDFSConnectionNodeFactory"/>
<node category-path="/" factory-class="de.bund.bfr.knime.hdfs.file.HDFSDownloadNodeFactory" id="de.bund.bfr.knime.hdfs.file.HDFSDownloadNodeFactory"/>
<node category-path="/" factory-class="de.bund.bfr.knime.hdfs.file.MergeHDFSFilesNodeFactory" id="de.bund.bfr.knime.hdfs.file.MergeHDFSFilesNodeFactory"/>
</extension>
   <extension point="org.knime.workbench.repository.categories">
      <category description="Flink connector" icon="src/de/bund/bfr/knime/flink/flink.png" level-id="flink" locked="true" name="Flink connector" path="/">
      </category>
      <category description="HDFS" icon="src/de/bund/bfr/knime/hdfs/hadoop.png" level-id="hdfs" locked="true" name="HDFS" path="/">
      </category>
   </extension>
   
</plugin>
