<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<?eclipse version="3.0"?><plugin>

   <!-- This is an example for providing an own category 
      <extension
         point="org.knime.workbench.repository.categories">
      <category
            description="Category containing my nodes"
            icon="icons/default.png"
            level-id="myNodes"
            name="My nodes"
            path="/"/>
   </extension>
   -->
   
   <extension point="org.knime.workbench.repository.nodes">
      <node category-path="/" factory-class="de.bund.bfr.knime.aaw.MyClusteringNodeFactory" id="de.bund.bfr.knime.aaw.MyClusteringNodeFactory"/>
   <node category-path="/" factory-class="de.bund.bfr.knime.aaw.hartung.MyImporterNodeFactory" id="de.bund.bfr.knime.aaw.hartung.MyImporterNodeFactory"/>
<node category-path="/" factory-class="de.bund.bfr.knime.aaw.lims.MyTab21NodeFactory" id="de.bund.bfr.knime.aaw.lims.MyTab21NodeFactory"/>
<node category-path="/" factory-class="de.bund.bfr.knime.aaw.lims.MyJavaMatcherNodeFactory" id="de.bund.bfr.knime.aaw.lims.MyJavaJoinerNodeFactory"/>
<node category-path="/" factory-class="de.bund.bfr.knime.aaw.lims.MyLIMSZooImporterNodeFactory" id="de.bund.bfr.knime.aaw.lims.MyLIMSZooImporterNodeFactory"/>
<node category-path="/" factory-class="de.bund.bfr.knime.aaw.json.TransformerNodeFactory" id="de.bund.bfr.knime.aaw.json.TransformerNodeFactory"/>
<node category-path="/" factory-class="de.bund.bfr.knime.aaw.json.MyJSON2TableNodeFactory" id="de.bund.bfr.knime.aaw.json.MyJSON2TableNodeFactory"/>
</extension>
   
</plugin>
